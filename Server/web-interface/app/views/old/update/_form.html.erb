<%= form_for @update, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :level, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :level, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :type, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :type, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :type_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :type_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :repository_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :repository_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :comit_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :commit_id, :class => 'number_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                update_path, :class => 'btn' %>
  </div>
<% end %>
